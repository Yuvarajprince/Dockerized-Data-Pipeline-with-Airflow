# --- Airflow ---
AIRFLOW_UID=50000
AIRFLOW_GID=0
AIRFLOW_USERNAME=admin
AIRFLOW_PASSWORD=admin@123
AIRFLOW_EMAIL=admin@example.com

# --- Postgres ---
system=PostgreSQL
Server=postgres
POSTGRES_USER=airflow
POSTGRES_PASSWORD=airflow@123
POSTGRES_DB=airflow

# --- Pipeline Config ---
ALPHAVANTAGE_API_KEY=  #replace me with your own API key
# Comma-separated symbols (keep it small for free tier rate limits)
SYMBOLS=AAPL,MSFT,GOOGL
# daily or intraday
AV_FUNCTION=TIME_SERIES_DAILY
# intraday interval required when AV_FUNCTION=TIME_SERIES_INTRADAY (e.g., 5min, 15min)
AV_INTERVAL=5min
# Cron: hourly at minute 5 (default). Examples: "@daily", "0 * * * *"
SCHEDULE_CRON=5 * * * *
# Timezone for Airflow DAG (IANA)
AIRFLOW_TZ=Asia/Kolkata

# --- Resilience ---
REQUEST_TIMEOUT=20
MAX_RETRIES=3
RETRY_DELAY_SECS=10